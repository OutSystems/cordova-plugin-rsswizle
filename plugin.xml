<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.chuckytuh.cordova.rsswizzle" version="1.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>RSSWizzle</name>
    
    <author>João Gonçalves</author>
    <license>MIT</license>
    
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="RSSWizzle">
                <param name="ios-package" value="RSSWizzle" />
            </feature>
        </config-file>
        <header-file src="src/ios/RSSwizzle.h" />
        <source-file src="src/ios/RSSwizzle.m" />
    </platform>
</plugin>